{
  "name": "Autossh-Mod",
  "version": "0.3.0",
  "slug": "autossh-mod",
  "description": "Automatically connect to a ssh server for forwarding ports",
  "url": "https://github.com/amirv/ha-autossh.git",
  "arch": ["armv7", "armhf", "amd64", "aarch64", "i386"],
  "startup": "application",
  "boot": "auto",
  "map": ["config:rw"],
  "options": {
    "hostname": "",
    "ssh_port": "22",
    "username": "autossh",
    "remote_forwarding": ["*:8123:172.17.0.1:8123"],
    "local_forwarding": [""],
    "other_ssh_options": "-v",
    "monitor_port": "0"
  },
  "schema": {
    "hostname": "str",
    "ssh_port": "int",
    "username": "str",
    "remote_forwarding": ["str"],
    "local_forwarding": ["str"],
    "other_ssh_options": "str",
    "monitor_port": "str"
  }
}
